{
    "manifest_version": 2,
    "name": "Test",
    "description": "This is a test",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./block_sites.js"]
        }
    ],

    "browser_action": { 
    "default_popup": "popup.html",
    "default_title": "Hackathon"
    },
    "permissions": [
        "tabs", 
        "alarms", 
        "cookies", 
        "debugger", 
        "webRequestBlocking", 
        "notifications", 
        "tts"
    ]
}
